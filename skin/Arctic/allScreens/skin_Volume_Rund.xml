<skin>
<screen backgroundColor="transparent" flags="wfNoBorder" name="Volume" position="1700,20" size="200,200" title="Volume" zPosition="10">
  
  <ePixmap name="" position="0,0" size="200,200" pixmap="Arctic/volume/volume-back_1.png" alphatest="blend" zPosition="10" />
  <widget name="Volume" position="0,0" size="0,0"/>


  <widget  position="0,50" source="global.CurrentTime" render="VVolumeText" font="Regular;40" size="200,150" transparent="1" zPosition="12" foregroundColor="#00FFFFFF" backgroundColor="#FFFFFFFF" halign="center" valign="center" />
	<widget path="volume/progress/" position="0,0"  alphatest="blend" render="RoundVolume" size="200,200" source="session.CurrentService" zPosition="11">
  		<convert type="ArcticVolume" />
	</widget>
</screen>
<screen backgroundColor="transparent" flags="wfNoBorder" name="Mute" position="1700,20" size="200,200" title="Mute" zPosition="10">
  
  <ePixmap name="" position="0,0" size="200,200" pixmap="Arctic/volume/volume-back.png" alphatest="blend" zPosition="10" />
  <ePixmap name="" position="0,0" size="200,200" pixmap="Arctic/volume/progress/mute.png" alphatest="blend" zPosition="11" />

</screen>
</skin>